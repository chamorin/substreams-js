{
  "name": "@substreams/sink",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "release": "pnpm build && ../../scripts/prerelease.sh ./package.json",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typings": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": ["dist/**", "!dist/**/.tsbuildinfo", "src/**/*.ts", "!src/**/*.test.ts"],
  "repository": {
    "type": "git",
    "url": "https://github.com/substreams-js/substreams-js.git",
    "directory": "packages/sink"
  },
  "dependencies": {
    "@bufbuild/connect": "^0.12.0",
    "@effect/data": "^0.17.1",
    "@effect/io": "^0.37.0",
    "@effect/match": "^0.31.0",
    "@effect/stream": "^0.32.1",
    "@substreams/core": "^0.1.17"
  },
  "devDependencies": {
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^0.33.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.1",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.33.0"
  }
}
